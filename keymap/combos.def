// Combos definitions 

//   name            result              chord keys

COMB( ZX_REDO,       LCTL(KC_Z),    KC_X, KC_Q)             // Undo
COMB( XC_CUT,        LCTL(KC_X),    KC_Q, KC_M)             // Cut
COMB( CD_COPY,       LCTL(KC_C),    KC_M, KC_W)             // Copy
COMB( DV_PASTE,      LCTL(KC_V),    KC_W, KC_Z)             // Paste

COMB( LD_ESC2,       KC_ESC,         KC_L, KC_D)            // ESC
COMB( BL_TAB2,       KC_TAB,         LGUI_T(KC_N), LALT_T(KC_R))     // TAB
COMB( HAE_ENT,       KC_ENT,         RSFT_T(KC_H), RCTL_T(KC_A), RALT_T(KC_E))     // ENT

COMB( RT_NAV,     MO(_NAV),      LALT_T(KC_R), LCTL_T(KC_T))     // TAB

COMB( CD_SMCL,       KC_SCLN,        KC_COMMA, KC_DOT)               // Semi Column
COMB( MUSIC,       KC_MSEL,        KC_VOLU, KC_VOLD)               // Semi Column

COMB( TS_CAPS,       KC_CAPS,        KC_D, KC_C)                     // Caps
COMB( GAMES,   TG(_GAMES),         KC_COMMA, KC_DOT, KC_SLSH) // Games Layer
COMB( QWERTY,   TG(_QWERTY),         KC_DOT, KC_SLSH) // Mouse Layer



